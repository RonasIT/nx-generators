{
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "commonjs",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@nx-generators/source": ["src/index.ts"],
      "@ronas-it/expo-app/shared/data-access/storage": [
        "libs/expo-app/shared/data-access/storage/src/index.ts"
      ],
      "@ronas-it/expo-app/shared/ui/styles": [
        "libs/expo-app/shared/ui/styles/src/index.ts"
      ],
      "@ronas-it/expo-app/shared/utils/app-env": [
        "libs/expo-app/shared/utils/app-env/src/index.ts"
      ],
      "@ronas-it/next-app/shared/utils/app-env": [
        "libs/next-app/shared/utils/app-env/src/index.ts"
      ],
      "@ronas-it/nx-generators": ["plugin/src/index.ts"],
      "@ronas-it/test/shared/utils/app-env": [
        "libs/test/shared/utils/app-env/src/index.ts"
      ]
    }
  },
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.lib.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ]
}
